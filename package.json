{
  "name": "chrome-extensions-template",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm uno:build && tsc -b && vite build",
    "format": "prettier --write \"./src/pages/**/*.{html,js,jsx,ts,tsx,json}\"",
    "lint": "eslint \"./src/**/*.{html,js,jsx,ts,tsx,json}\" --fix --max-warnings=0",
    "prepare": "husky",
    "start": "nodemon",
    "stylelint": "stylelint \"./src/pages/**/*.{css,less}\" --fix",
    "uno:build": "unocss \"./src/pages/**/*\" -o ./src/assets/styles/uno.css"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "webextension-polyfill": "^0.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@commitlint/types": "^19.0.3",
    "@crxjs/vite-plugin": "2.0.0-beta.25",
    "@eslint/js": "^9.9.0",
    "@stylistic/stylelint-plugin": "^3.0.1",
    "@types/chrome": "^0.0.270",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/webextension-polyfill": "^0.12.1",
    "@unocss/cli": "^0.62.3",
    "@unocss/preset-wind": "^0.62.3",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "husky": "^9.1.5",
    "less": "^4.2.0",
    "lint-staged": "^15.2.9",
    "nodemon": "^3.1.4",
    "postcss-less": "^6.0.0",
    "prettier": "^3.3.3",
    "prettier-plugin-organize-imports": "^4.0.0",
    "prettier-plugin-packagejson": "^2.5.2",
    "stylelint": "^16.9.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-less": "^3.0.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-order": "^6.0.4",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "unocss": "^0.62.3",
    "vite": "^5.4.1"
  }
}
